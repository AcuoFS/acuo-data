LOAD CSV WITH HEADERS FROM 'https://raw.githubusercontent.com/AcuoFS/acuo-data/master/CSV/margincall/mstatement.csv' 
AS line

MATCH (a:Agreement {id:line.AgreementId})

MERGE (ms:MarginStatement {id:line.StatementId, date:line.StatementDate, interestPayment:toFloat(line.InterestPayment), productCashFlows:toFloat(line.ProductCashFlows), PAI:toFloat(line.PAI), feesCommissions:toFloat(line.FeesCommissions), pendingCash:toFloat(line.PendingCash), pendingNonCash:toFloat(linePendingNonCash), totalAmount:toFloat(line.TotalAmount)})

MERGE (ms)-[:STEMS_FROM]->(a)
