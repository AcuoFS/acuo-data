LOAD CSV WITH HEADERS FROM '%dataImportLink%/csv/custodians.csv' 
AS line

MERGE (cust: Custodian {id:line.custodianId})
ON MATCH SET
	cust.name = line.custodianName,
	cust.country = line.country,
	cust.countryShortName = line.countryShortName
ON CREATE SET
	cust.name = line.custodianName,
	cust.country = line.country,
	cust.countryShortName = line.countryShortName
