LOAD CSV WITH HEADERS FROM '%dataImportLink%/csv/fxRates.csv'
AS line

MATCH (c1:Currency {id:line.CurrencyISO})

WITH *

MATCH (c2:Currency {id:'USD'})

MERGE (c1)<-[:FROM]-(fxRate:FXRate {value:toFloat(line.ValueToUSD)})-[:TO]->(c2)
MERGE (value:FXValue {value:toFloat(line.ValueToUSD), from:toInteger(timestamp()/1000)})-[:OF]->(fxRate)
MERGE (fxRate)-[:LAST]->(value)